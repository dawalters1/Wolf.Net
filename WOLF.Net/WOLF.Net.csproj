<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <OutputType>Library</OutputType>
    <Version>4.0.0-alpha2</Version>
    <Description>Unofficial C# library for connecting to WOLF (AKA Palringo)</Description>
    <PackageTags>WOLF, Palringo, Pal, Wolfringo</PackageTags>
    <RepositoryUrl>https://github.com/dawalters1/Wolf.Net</RepositoryUrl>
    <PackageIcon>icon.png</PackageIcon>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <ApplicationIcon>icon_vgE_icon.ico</ApplicationIcon>
    <PackageReleaseNotes>- Removed Console.WriteLine
- Fixed issue with subCode parsing</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="SocketIoClient" Version="2.1.1" />
    <PackageReference Include="System.Drawing.Common" Version="4.7.0" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Entities\Groups\Events\" />
    <Folder Include="Entities\Tipping\Leaderboards\" />
  </ItemGroup>

  <ItemGroup>
    <None Include="C:\Users\Dave\Downloads\icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
